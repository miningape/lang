let fib = (n: number): number =>
    if n <= 1
        n
    else
        fib(n - 1) + fib(n - 2);

let factorial = (n: number): number => 
    if n == 1
        n
    else 
        n * factorial(n - 1);

let add = (a: any, b: any): string | number
    => a + b;

print("Fibonacci 6: ", fib(6));
print("Factorial 6: ", factorial(6));
print(add(1, 2));
